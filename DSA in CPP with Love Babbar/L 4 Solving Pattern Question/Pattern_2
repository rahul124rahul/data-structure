/* 
for n=5

5 4 3 2 1
5 4 3 2 1
5 4 3 2 1
5 4 3 2 1
5 4 3 2 1

 */

#include<iostream>
using namespace std;

int main(){
    int n;
    cin>>n;
    int i=1;
    while(i<=n){
        int j=n;
        while(j>0){
            cout<<j<<" ";
            j--;
        }
        cout<<endl;
        i++;
    }
    
    return 0;
}


Output :

8

8 7 6 5 4 3 2 1 
8 7 6 5 4 3 2 1 
8 7 6 5 4 3 2 1 
8 7 6 5 4 3 2 1 
8 7 6 5 4 3 2 1 
8 7 6 5 4 3 2 1 
8 7 6 5 4 3 2 1 
8 7 6 5 4 3 2 1
