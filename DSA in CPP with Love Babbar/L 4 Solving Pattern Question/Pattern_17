

/*
for n = 4

D 
C D 
B C D 
A B C D 

  */

#include<iostream>
using namespace std;

int main(){

    int n;
    cin>>n;
    int i=1;
    while(i<=n){
        int j=1;
        while(j<=i){
            cout<<char(64+n-i+j)<<" ";
            j++;
        }
        cout<<endl;
        i++;
    }

    
    return 0;
}



