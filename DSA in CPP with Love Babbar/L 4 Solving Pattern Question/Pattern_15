
/*
for n=4

A 
B C 
C D E 
D E F G 

*/
#include<iostream>
using namespace std;

int main(){
    int n;
    cin>>n;
    int i=1;
    int asc=65;

    while(i<=n){
        int j=1;
        while(j<=i){
            cout<<char(asc+i+j-2)<<" ";
            j++;
        }
        cout<<endl;
        i++;
    }
    
    return 0;
}



Output :



