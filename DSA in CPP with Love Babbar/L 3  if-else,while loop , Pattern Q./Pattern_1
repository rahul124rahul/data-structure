/*  
for n=4

* * * * 
* * * * 
* * * * 
* * * * 

*/

// Again practice the codes for being proficient in the things in programming

#include<iostream>
using namespace std;

int main(){
    int n;
    cin>>n;
    int i=1;
    while(i<=n){
        int j=1;
        while(j<=n){
            cout<<"* ";
            j++;
        }
        cout<<endl;
        i++;
    }
    
    return 0;
}




Output :


6
* * * * * * 
* * * * * * 
* * * * * * 
* * * * * * 
* * * * * * 
* * * * * * 


