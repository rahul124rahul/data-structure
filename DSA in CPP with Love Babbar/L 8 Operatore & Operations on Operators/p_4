/* 

To Write a function for returning the b power of a if pow(a,b) is given 

ie. pow(a,b) = a^b */

#include<iostream>
using namespace std;


int power(int a, int b){
    int p=1;
    for(int i=0 ; i<b ; i++){
        p=p*a;
    }
    return p;
}


int main(){
    int a,b;
    cin>>a>>b;

    int res = power(a,b);

    cout<<res<<endl;
    
    
    return 0;
}




Output :


2 5
32


76 4
33362176


66 7
552235136
